{
  "title": "SPMouseless",
  "name": "spmouseless",
  "version": "0.4.1",
  "description": "SharePoint navigation keyboard shourtcuts",
  "main": "./lib/index.js",
  "author": "dstarkowski",
  "engines": {
    "firefox": ">=38.0a1"
  },
  "preferences": [{
    "name": "theme",
    "title": "Color theme",
    "description": "Requires restart",
    "type": "radio",
    "value": 1,
    "options": [{
      "value": 0,
      "label": "Light"
    },
    {
      "value": 1,
      "label": "Dark"
    }]
  }],
  "license": "MIT",
  "permissions": {
    "private-browsing": true
  },
  "keywords": [
    "jetpack"
  ],
  "dependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  }
}